{% extends 'ItesACBackendBundle::layout.html.twig' %}
{% block encabezado %}Arduino{% endblock %}
{% block cuerpo %}
<div class="table-responsive">
    <table class="record_properties table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ entity.ip }}</td>
            </tr>
        </tbody>
    </table>
</div>
<div class="record_actions row">
    <div class="btn-group col-xs-8 col-sm-8 col-md-8">
        <a href="{{ path('arduino') }}" class="btn btn-default" role="button">
            Back to the list
        </a>
        <a href="{{ path('arduino_edit', { 'id': entity.id }) }}" class="btn btn-default" role="button">
            Edit
        </a>
    </div>
    <div class="col-xs-4 col-sm-4 col-md-4">
        {{ form(delete_form) }}
    </div>
</div>
{% endblock %}
